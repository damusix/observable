{
    "compilerOptions": {
        "lib": ["es2015", "es2016", "es2017", "dom"],
        "target": "ESNext",
        "module": "esnext",
        "strict": true,
        "sourceMap": true,
        "noUnusedLocals": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "declaration": true
    },
    "include": [
        "lib"
    ]
}